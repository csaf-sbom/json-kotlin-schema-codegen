{
  "$schema": "http://json-schema.org/draft/2019-09/schema",
  "$id": "http://pwall.net/test-default",
  "title": "Dummy",
  "description": "Test default values.",
  "type": "object",
  "properties": {
    "aaa": {
      "description": "Integer value.",
      "type": "integer",
      "default": 0
    },
    "bbb": {
      "description": "Nullable string value.",
      "type": [ "string", "null" ],
      "default": null
    },
    "ccc": {
      "description": "String value.",
      "type": "string",
      "default": "CCC"
    },
    "ddd": {
      "description": "Array value.",
      "type": "array",
      "items": {
        "type": "integer"
      },
      "default": [ 123, 456 ]
    },
    "eee": {
      "description": "Date-time value.",
      "type": "string",
      "format": "date-time",
      "default": "2021-09-22T10:26:09.123+10:00"
    },
    "fff": {
      "description": "Date value.",
      "type": "string",
      "format": "date",
      "default": "2021-09-22"
    },
    "ggg": {
      "description": "Time value.",
      "type": "string",
      "format": "time",
      "default": "10:27:26"
    },
    "hhh": {
      "description": "URI value.",
      "type": "string",
      "format": "uri",
      "default": "http://json-schema.org/draft/2019-09/schema"
    },
    "iii": {
      "description": "UUID value.",
      "type": "string",
      "format": "uuid",
      "default": "9be4c4b6-1b37-11ec-b9d6-c71d791f2a0a"
    },
    "jjj": {
      "description": "String value.",
      "type": "string",
      "default": "xyz"
    },
    "kkk": {
      "description": "Enum array value.",
      "type": "array",
      "items": {
        "description": "Enum value.",
        "type": "string",
        "enum": [ "ABC", "XYZ" ]
      },
      "default": [ "ABC" ]
    }
  }
}
